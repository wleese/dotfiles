" Ctrl-a
inoremap <C-a> <C-Home><C-S-End>

" default search/suggest engine: google
set defsearch=google
set suggestengines=google

" don't focus any "input" elements automatically when open pages.
set focuscontent

ignorekeys add gmail.com
ignorekeys add mail.google.com
ignorekeys add confluence.tools.bol.com
ignorekeys add jira.tools.bol.com
ignorekeys add hipchat.com
ignorekeys add docs.google.com
